{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "shell.ui.main.Main",
            "projectName": "IXDAR",
            "args": "djbouti",
            "vmArgs": "-enableassertions"
        },
        {
            "type": "java",
            "name": "Ixdar",
            "request": "launch",
            "mainClass": "shell.ui.IxdarWindow",
            "projectName": "IXDAR",
            "vmArgs": "-enableassertions -Dsun.awt.noerasebackground=true -Dorg.lwjgl.util.DebugLoader=true"
        },
        {
            "type": "java",
            "name": "Bouncing Line",
            "request": "launch",
            "mainClass": "shell.ui.IxdarWindow",
            "projectName": "IXDAR",
            "args": "bouncing-line-canvas",
            "vmArgs": "-enableassertions -Dsun.awt.noerasebackground=true -Dorg.lwjgl.util.DebugLoader=true"
        },
        {
            "type": "java",
            "name": "Circle",
            "request": "launch",
            "mainClass": "shell.ui.IxdarWindow",
            "projectName": "IXDAR",
            "args": "circle-canvas",
            "vmArgs": "-enableassertions -Dsun.awt.noerasebackground=true -Dorg.lwjgl.util.DebugLoader=true"
        },
        {
            "type": "java",
            "name": "Line",
            "request": "launch",
            "mainClass": "shell.ui.IxdarWindow",
            "projectName": "IXDAR",
            "args": "line-canvas",
            "vmArgs": "-enableassertions -Dsun.awt.noerasebackground=true -Dorg.lwjgl.util.DebugLoader=true"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "vmArgs": "-enableassertions",
            "console": "internalConsole",
        },
        {
            "type": "java",
            "name": "Launch Profile",
            "request": "launch",
            "mainClass": "shell.Main",
            "projectName": "IXDAR",
            "vmArgs": "-enableassertions -agentpath:Z:/VisualVM/visualvm_219/visualvm/lib/deployed/jdk16/windows-amd64/profilerinterface.dll=Z:\\VisualVM\\visualvm_219\\visualvm\\lib,5140"
        },
        {
            "type": "java",
            "name": "Web (TeaVM): build",
            "request": "launch",
            "mainClass": "shell.ui.WebLauncher",
            "projectName": "IXDAR",
            "preLaunchTask": "teavm-build"
        },
        {
            "type": "pwa-msedge",
            "request": "launch",
            "name": "Web (TeaVM): open dev server",
            "preLaunchTask": "serve-teavm",
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}/target/teavm"
        }
    ]
}